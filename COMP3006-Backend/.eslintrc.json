{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["google", "prettier"],
    "plugins": ["prettier"],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "requireConfigFile": false
    },
    "rules": {
        "prettier/prettier": ["error"]
    }
}
